%%ETCDIR%%/infobot.channels
@unexec if cmp -s %D/%%ETCDIR%%/infobot.config %D/%%ETCDIR%%/infobot.config-dist; then rm -f %D/%%ETCDIR%%/infobot.config; fi
%%ETCDIR%%/infobot.config-dist
